<%= f.fields_for :artist_attributes do |builder| %>
    <%= builder.hidden_field :user_id, :value => user.id %>
    <%= builder.label :name, 'Artist' %><br>
    <%= builder.text_field :name, list: 'artist-name' %>
    <datalist id='artist-name'>
    <% user.unique_artists.each do |artist| %>
        <option value="<%= artist.name %>"></option>
    <% end %>
    </datalist>
<% end %>