$('#setlist_list').children().remove();
<% @setlist.set_entries.order(position: :asc).each do |set_entry| %>
    $('#setlist_list').append('<%= j render partial: "arrangements/arrangement_card", :locals => { user: @user, arrangement: set_entry.arrangement, kind: "setlist_list", set_entry: set_entry } %>')
<% end %>
