<%= f.fields_for :arranger_attributes do |builder| %>
    <%= builder.hidden_field :user_id, :value => user.id %>
    <%= builder.label :name, 'Arranger' %><br>
    <%= builder.text_field :name, list: 'arranger-name' %>
    <datalist id='arranger-name'>
    <% user.unique_arrangers.each do |arranger| %>
        <option value="<%= arranger.name %>"></option>
    <% end %>
    </datalist>
<% end %>